{
  "google": {
    "name": "Google Gemini",
    "description": "Google's Gemini AI models for creative writing and brainstorming",
    "baseUrl": "https://generativelanguage.googleapis.com",
    "modelsEndpoint": "/v1/models",
    "chatEndpoint": "/v1/models/{model}:generateContent",
    "keyRequired": true,
    "keyPlaceholder": "Enter your Google AI API key...",
    "keyHelpUrl": "https://aistudio.google.com/app/apikey",
    "keyHelpText": "Get your free API key from Google AI Studio",
    
    "fallbackModels": [
      {
        "value": "gemini-2.0-flash-exp",
        "label": "Gemini 2.0 Flash (Experimental)",
        "description": "Latest experimental model with enhanced creative capabilities",
        "contextLength": 1000000,
        "recommended": true
      },
      {
        "value": "gemini-1.5-pro",
        "label": "Gemini 1.5 Pro", 
        "description": "High-quality model for complex creative tasks",
        "contextLength": 2000000,
        "premium": true
      },
      {
        "value": "gemini-1.5-flash",
        "label": "Gemini 1.5 Flash",
        "description": "Fast, efficient model for creative writing",
        "contextLength": 1000000,
        "recommended": false
      },
      {
        "value": "gemini-1.5-flash-8b",
        "label": "Gemini 1.5 Flash 8B",
        "description": "Lightweight model for quick brainstorming",
        "contextLength": 1000000,
        "recommended": false
      }
    ],

    "apiLimits": {
      "maxChatHistoryMessages": 20,
      "maxChatHistoryTokens": 20000,
      "estimatedTokensPerChar": 0.3
    },
    
    "defaultGenerationConfig": {
      "temperature": 0.7,
      "maxOutputTokens": 2048,
      "topP": 0.9,
      "topK": 40,
      "candidateCount": 1
    },
    
    "creativeGenerationConfig": {
      "temperature": 0.9,
      "maxOutputTokens": 2048,
      "topP": 0.95,
      "topK": 50,
      "candidateCount": 1
    },
    
    "analyticalGenerationConfig": {
      "temperature": 0.3,
      "maxOutputTokens": 2048,
      "topP": 0.8,
      "topK": 20,
      "candidateCount": 1
    }
  },
  
