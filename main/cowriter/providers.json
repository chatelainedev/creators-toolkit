  "meta": {
    "providerSettings": {
      "defaultProvider": "google",
      "defaultModel": "gemini-2.0-flash-exp",
      "maxRetries": 3,
      "timeout": 30000,
      "maxPromptLength": 50000,
      "maxChatHistory": 20
    },
    
    "uiSettings": {
      "showProviderDescriptions": true,
      "showModelContextLengths": true,
      "showPremiumIndicators": true,
      "groupModelsByCapability": false
    },
    
    "rateLimiting": {
      "enabled": false,
      "requestsPerMinute": 60,
      "requestsPerHour": 1000
